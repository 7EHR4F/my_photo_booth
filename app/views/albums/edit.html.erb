<h1>Edit Album</h1>
 
<% form_for @album do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "form", :object => f %>
  <%= f.submit "Update" %>
<% end %>
 
<br /><%= link_to("Delete album", { :action => "destroy", :id => @album },
  :confirm => "Are you sure you want to delete this album?",
  :method => :delete) %>

<br /><%= link_to "All albums", albums_path %>